# Frontend environment variables for production
# .env.production

# API Configuration
NEXT_PUBLIC_API_URL=https://api.yourtradingapp.com
NEXT_PUBLIC_WEBSOCKET_URL=wss://api.yourtradingapp.com/ws

# Feature Flags
NEXT_PUBLIC_ENABLE_ALGORITHMIC_TRADING=true
NEXT_PUBLIC_ENABLE_OPTIONS_STRATEGY_BUILDER=true

# Analytics
NEXT_PUBLIC_ANALYTICS_ID=UA-PRODUCTION-ID

# General
NEXT_PUBLIC_ENVIRONMENT=production
NEXT_PUBLIC_DEPLOYMENT_VERSION=${GITHUB_SHA}

# Mock Data (for production environment)
NEXT_PUBLIC_USE_MOCK_DATA=false

# Cache Control
NEXT_PUBLIC_CACHE_MAX_AGE=86400
