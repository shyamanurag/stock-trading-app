# Frontend environment variables for staging
# .env.staging

# API Configuration
NEXT_PUBLIC_API_URL=https://api-staging.yourtradingapp.com
NEXT_PUBLIC_WEBSOCKET_URL=wss://api-staging.yourtradingapp.com/ws

# Feature Flags
NEXT_PUBLIC_ENABLE_ALGORITHMIC_TRADING=true
NEXT_PUBLIC_ENABLE_OPTIONS_STRATEGY_BUILDER=true

# Analytics
NEXT_PUBLIC_ANALYTICS_ID=UA-STAGING-ID

# General
NEXT_PUBLIC_ENVIRONMENT=staging
NEXT_PUBLIC_DEPLOYMENT_VERSION=${GITHUB_SHA}

# Mock Data (for staging environment)
NEXT_PUBLIC_USE_MOCK_DATA=false

# Cache Control
NEXT_PUBLIC_CACHE_MAX_AGE=3600
